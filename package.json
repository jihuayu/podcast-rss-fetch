    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "drizzle-kit": "^0.20.6",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
