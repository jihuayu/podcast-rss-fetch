{
  "name": "podcast-rss-fetch-ts",
  "version": "1.0.0",
  "description": "TypeScript version of podcast RSS fetcher with Drizzle ORM",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "tsx src/main.ts",
